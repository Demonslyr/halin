  "version": "0.8.1",
  "neo4jDesktop": {
    "apiVersion": "^1.2.0"
  },
  "license": "Apache-2.0",
  "icons": [
    {
      "src": "./public/img/halin-icon.png",
      "type": "png"
    }
  ],
  "repository": {
    "type": "git",
