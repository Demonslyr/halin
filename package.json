  },
  "homepage": "http://halin.graphapp.io/",
  "private": false,
  "dependencies": {
    "@sentry/browser": "^4.4.1",
    "autobind-decorator": "2.2.1",
    "bluebird": "^3.5.3",
    "graph-app-kit": "^1.0.3",
    "lodash": "^4.17.11",
    "mathjs": "^5.4.0",
    "moment": "^2.22.2",
    "neo4j-driver": "^1.7.2",
    "pondjs": "^0.8.9",
    "prop-types": "^15.6.0",
    "react": "^16.6.3",
    "react-d3-components": "^0.9.1",
    "react-dom": "^16.6.3",
    "react-graph-vis": "^1.0.2",
    "react-scripts": "1.0.16",
    "react-sortable-tree": "^2.2.0",
    "react-table": "^6.8.6",
    "react-timeseries-charts": "^0.15.3",
